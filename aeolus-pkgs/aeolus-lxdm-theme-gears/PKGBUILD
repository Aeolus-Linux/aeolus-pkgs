pkgname=aeolus-lxdm-theme-gears
pkgver=1.0
pkgrel=1
pkgdesc="A gears styled lxdm theme for Aeolus."
arch=('any')
license=('GPL')
provides=($pkgname)
conflicts=($pkgname)
depends=('lxdm-gtk3' 'adapta-gtk-theme')

package() {
	install -D -m644 aeolus-gears/* -t $pkgdir/usr/share/lxdm/themes/aeolus-gears
}
