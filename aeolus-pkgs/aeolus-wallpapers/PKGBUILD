pkgname=aeolus-wallpapers
pkgver=1.0
pkgrel=1
pkgdesc="Aeolus wallpaper pack."
arch=('any')
license=('GPL')
provides=($pkgname)
conflicts=($pkgname)
depends=()

package() {
        mkdir -p $pkgdir/usr/share/backgrounds/"
	cp -r * "$pkgdir/usr/share/backgrounds/"
}
	