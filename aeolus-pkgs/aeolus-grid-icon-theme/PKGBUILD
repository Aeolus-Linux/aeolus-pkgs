pkgname=aeolus-grid-icon-theme
pkgver=1.0
pkgrel=1
pkgdesc="Grid icon theme for Aeolus."
arch=('any')
license=('GPL')
provides=($pkgname)
conflicts=($pkgname)
depends=()

package() {
	mkdir -p "$pkgdir/usr/share/icons"
	cp -r grid "$pkgdir/usr/share/icons/"
}
