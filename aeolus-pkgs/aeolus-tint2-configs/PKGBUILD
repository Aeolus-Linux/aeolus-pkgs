# Maintainer: Eile Kerning <tysayahi@gmail.com>

pkgname=aeolus-tint2-configs
pkgver=1.0
pkgrel=1
pkgdesc="Aeolus tint2 configs.."
arch=('any')
license=('GPL')
provides=($pkgname)
conflicts=($pkgname)
depends=()

package() {
	# copy whole dir recursively to destination
	(find aeolus -type f -exec install -Dm 644 "{}" "$pkgdir/usr/share/tint2/{}" \;)
}
