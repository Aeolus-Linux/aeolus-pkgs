pkgname=aeolus-tint2-configs
pkgver=1.0
pkgrel=1
pkgdesc="Aeolus tint2 configs.."
arch=('any')
license=('GPL')
provides=($pkgname)
conflicts=($pkgname)
depends=()

package() {
	mkdir -p $pkgdir/usr/share/tint2/ && cp -r * "$pkgdir/usr/share/tint2/"
}
